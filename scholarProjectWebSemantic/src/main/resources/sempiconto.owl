    <!-- http://miashs.univ-grenoble-alpes.fr/ontologies/sempic.owl#Album -->

    <owl:Class rdf:about="http://miashs.univ-grenoble-alpes.fr/ontologies/sempic.owl#Album">
        <rdfs:subClassOf rdf:resource="http://miashs.univ-grenoble-alpes.fr/ontologies/sempic.owl#AlbumPhoto"/>
        <rdfs:comment xml:lang="en">An album is defined by its URI id. During the life of an Album, its title can change but not its ID.</rdfs:comment>
        <rdfs:label xml:lang="en">Album</rdfs:label>
    </owl:Class>



    <!-- http://miashs.univ-grenoble-alpes.fr/ontologies/sempic.owl#AlbumPhoto -->

    <owl:Class rdf:about="http://miashs.univ-grenoble-alpes.fr/ontologies/sempic.owl#AlbumPhoto">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://miashs.univ-grenoble-alpes.fr/ontologies/sempic.owl#Album"/>
            <rdf:Description rdf:about="http://miashs.univ-grenoble-alpes.fr/ontologies/sempic.owl#Photo"/>
        </owl:disjointUnionOf>
        <rdfs:comment xml:lang="en">We may manage several Albums thanks the app (TODO not implemented)</rdfs:comment>
    </owl:Class>



